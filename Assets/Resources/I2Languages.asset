        \u0442\u0435\u0440\u0440\u0438\u0442\u043E\u0440\u0438\u0438. \u041E\u043D\u0438
        \u043E\u0442\u043A\u0440\u044B\u0432\u0430\u044E\u0442\u0441\u044F, \u043A\u043E\u0433\u0434\u0430
        \u043D\u0430\u0445\u043E\u0434\u044F\u0442\u0441\u044F \u0432 \u043F\u043E\u043B\u0435
        \u0437\u0440\u0435\u043D\u0438\u044F <sprite=39> \u0432\u0430\u0448\u0435\u0433\u043E
